{
    "files.exclude": {
        "**/node_modules": true,
        "**/package-lock.json": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/package-lock.json": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/package-lock.json": true
    },
    "cSpell.enabled": false
}
